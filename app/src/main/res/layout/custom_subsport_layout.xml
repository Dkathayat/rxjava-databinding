    <!--        app:layout_constraintEnd_toEndOf="@+id/makemodelTextInputLayout"-->
    <!--        app:layout_constraintTop_toTopOf="@+id/makemodelTextInputLayout" />-->
    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="@dimen/dimen_20"
        android:layout_height="@dimen/dimen_20"
        android:layout_marginStart="@dimen/dimen_20"
        android:src="@drawable/ic_cross"
        app:layout_constraintBottom_toBottomOf="@+id/makemodelTextInputLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/makemodelTextInputLayout"
        app:layout_constraintTop_toTopOf="@+id/makemodelTextInputLayout" />

</androidx.constraintlayout.widget.ConstraintLayout>